{
  "name": "vue",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "*",
    "autoprefixer": "*",
    "browser-sync": "*",
    "del": "^7.0.0",
    "eslint": "*",
    "eslint-config-htmlacademy": "*",
    "gulp": "*",
    "gulp-csso": "*",
    "gulp-imagemin": "*",
    "gulp-newer": "*",
    "gulp-plumber": "*",
    "gulp-postcss": "*",
    "gulp-posthtml": "*",
    "gulp-rename": "*",
    "gulp-sass": "*",
    "gulp-svgmin": "*",
    "gulp-svgstore": "*",
    "postcss-object-fit-images": "*",
    "stylelint": "*",
    "stylelint-config-htmlacademy": "*"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint \"src/js/**/*.js\"",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/modules/*.js",
    "src/sass/*.{sass,scss}",
    "src/sass/general/**/*.{sass,scss}",
    "src/sass/blocks/**/*.{sass,scss}"
  ],
  "eslintConfig": {
    "extends": "htmlacademy/es5"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ],
  "license": "ISC",
  "dependencies": {
    "sass": "^1.54.1"
  }
}
